{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "convert.image-cut.com"
        }
      ],
      "destination": "/converter.html"
    },
    {
      "source": "/((?!api|_next/static|_next/image|assets|favicon.ico).*)",
      "has": [
        {
          "type": "host",
          "value": "convert.image-cut.com"
        }
      ],
      "destination": "/converter.html"
    },
    {
      "source": "/PDFconvert",
      "has": [
        {
          "type": "host",
          "value": "image-cut.com"
        }
      ],
      "destination": "/pdfconv.html"
    },
    {
      "source": "/((?!api|_next/static|_next/image|assets|favicon.ico).*)",
      "has": [
        {
          "type": "host",
          "value": "image-cut.com"
        }
      ],
      "destination": "/index.html"
    }
  ]
}
