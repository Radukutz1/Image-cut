{
  "version": 2,
  "rewrites": [
    {
      "source": "/compress",
      "destination": "/compress.html"
    },
    {
      "source": "/convert",
      "destination": "/converter.html"
    },
    {
      "source": "/pdfconvert",
      "destination": "/pdfconv.html"
    },
    {
      "source": "/privacy",
      "destination": "/privacy.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/PDFconvert",
      "destination": "/pdfconvert",
      "permanent": true
    },
    {
      "source": "/converter.html",
      "destination": "/convert",
      "permanent": true
    },
    {
      "source": "/compress.html",
      "destination": "/compress",
      "permanent": true
    },
    {
      "source": "/pdfconv.html",
      "destination": "/pdfconvert",
      "permanent": true
    },
    {
      "source": "/privacy.html",
      "destination": "/privacy",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
}
