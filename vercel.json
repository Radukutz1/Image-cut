{
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "image-cut.com" }],
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "convert.image-cut.com" }],
      "destination": "/converter.html"
    }
  ],
  "redirects": [
    {
      "source": "https://www.image-cut.com/(.*)",
      "destination": "https://image-cut.com/$1",
      "permanent": true
    }
  ]
}
