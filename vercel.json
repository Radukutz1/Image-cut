{
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "image-cut.com" }],
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "convert.image-cut.com" }],
      "destination": "/converter.html"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.image-cut.com" }],
      "destination": "https://image-cut.com/:splat",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.convert.image-cut.com" }],
      "destination": "https://convert.image-cut.com/:splat",
      "permanent": true
    }
  ]
}
