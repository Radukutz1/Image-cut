{
  "version": 2,
  "redirects": [
    {
      "source": "/",
      "destination": "/",
      "permanent": false,
      "has": [
        {
          "type": "host",
          "value": "convert.image-cut.com"
        }
      ]
    },
    {
      "source": "/PDFconvert",
      "destination": "/pdfconvert",
      "permanent": true
    },
    {
      "source": "/PDFconvert/",
      "destination": "/pdfconvert",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/compress",
      "destination": "/compress.html"
    },
    {
      "source": "/convert",
      "destination": "/convert.html"
    },
    {
      "source": "/pdfconvert",
      "destination": "/pdfconvert.html"
    },
    {
      "source": "/privacy",
      "destination": "/privacy.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html",
      "has": [
        {
          "type": "host",
          "value": "image-cut.com"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    }
  ]
}
