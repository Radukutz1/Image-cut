{
  "version": 2,
  "routes": [
    {
      "src": "/PDFconvert",
      "has": [{ "type": "host", "value": "image-cut.com" }],
      "dest": "/pdfconv.html"
    },
    {
      "src": "/(.*)",
      "has": [{ "type": "host", "value": "image-cut.com" }],
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "has": [{ "type": "host", "value": "convert.image-cut.com" }],
      "dest": "/converter.html"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    }
  ]
}
